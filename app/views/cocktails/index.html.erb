<h1>Welcome to Your Cocktails</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @cocktails.each do |cocktail| %>
    <tr>
      <td><%= cocktail.name %></td>
      <td><%= link_to 'Show', cocktail %></td>
    </tr>
    <% end %>
  </tbody>
</table>
<hr>
<%= link_to 'New Cocktail', new_cocktail_path %>
<hr>

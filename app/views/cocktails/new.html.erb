<!-- I reference @cocktail from the cocktails_controller. it needs to be connected to our models.
  -->
<%= simple_form_for(@cocktail) do |f| %>
  <%= f.input :name %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.button :submit %>
<% end %>

<%= link_to 'Back', root_path %>
